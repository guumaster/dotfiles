<html>
<head>
    <title>{% block title %} untitled {% endblock %} </title>
        <!--[if lt IE 9]>
            <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
    {% block stylesheets %}
        <link href='http://fonts.googleapis.com/css?family=Irish+Grover' rel='stylesheet' type='text/css'>
        <link href='http://fonts.googleapis.com/css?family=La+Belle+Aurore' rel='stylesheet' type='text/css'>
        <link href="{{ asset('css/pruebas.css') }}" type="text/css" rel="stylesheet" />
    {% endblock %}
</head>
{% spaceless %}
<body>

<section id="wrapper">
    <header id="header">
        <div class="top">
            {% block navigation %} empty navigation {% endblock %}
        </div>
        <hgroup>
              <h2>{% block header %}<a href="#">Titulo</a>{% endblock %}</h2>
              <h3>{% block tagline %}<a href="#">Todas las pruebas</a>{% endblock %}</h3>
        </hgroup>

    </header>
    <section class="main-col">
        {% block body %} no content {% endblock %}
    </section>
    <aside class="sidebar">
                {% block sidebar %} sidebar {% endblock %}
    </aside>

    <div id="footer">
       {% block footer %}
           Texto footer
       {% endblock %}
    </div>

</section>
{% block javascript %} {% endblock %}

</body>
{% endspaceless %}
</html>
